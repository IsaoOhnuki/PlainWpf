<UserControl x:Class="PlainWpf.Views.DatabasePage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PlainWpf.Views"
             xmlns:ViewModels="clr-namespace:PlainWpf.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Background="#FFE8E8E8">
    <UserControl.DataContext>
        <ViewModels:DatabasePageViewModel/>
    </UserControl.DataContext>
    <Grid>
        <Button Content="Database" HorizontalAlignment="Left" Margin="95,86,0,0" VerticalAlignment="Top" Width="75" Padding="5" Command="{Binding ShowDatabaseDialogCommand}"/>
    </Grid>
</UserControl>
