<UserControl x:Class="PlainWpf.Views.SubPage2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PlainWpf.Views"
      xmlns:ViewModels="clr-namespace:PlainWpf.ViewModels"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      >
    <UserControl.DataContext>
        <ViewModels:SabPage1ViewModel/>
    </UserControl.DataContext>
    <Grid RenderTransformOrigin="0.5,0.5">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="40"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="40"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="40"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="40"/>
        </Grid.RowDefinitions>
        <Button Content="3" Grid.Column="0" Grid.Row="0"/>
        <Button Content="4" Grid.Column="2" Grid.Row="0"/>
        <Button Content="5" Grid.Column="0" Grid.Row="2"/>
    </Grid>
</UserControl>
